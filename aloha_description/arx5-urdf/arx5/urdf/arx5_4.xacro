  <xacro:macro name="arx5_transmission" params="prefix hard_interface reduction:=1">

    <transmission name="${prefix}tran0">
      <type>transmission_interface/SimpleTransmission</type>
      <joint name="${prefix}">
        <hardwareInterface>hardware_interface/${hard_interface}</hardwareInterface>
      </joint>
      <actuator name="${prefix}motor0">
        <hardwareInterface>hardware_interface/${hard_interface}</hardwareInterface>
        <mechanicalReduction>${reduction}</mechanicalReduction>
      </actuator>
    </transmission>
  </xacro:macro>

    <xacro:arx5_transmission prefix="arm4_joint0" hard_interface="PositionJointInterface">
